mosquitto_pub -h host -t beehive/weight -m "{\"id\":123, \"weight\":5.42}" -p 1883 -u user -P password

moquitto_pub -h host -t beehive/battery_percentage -m "{\"id\":123, \"battery_percentage\":42}" -p 1883 -u user -P password

mosquitto_pub -h host -t beehive/location -m "{\"id\":123, \"longitude\":5.42, \"latitude\":6.42}" -p 1883 -u user -P password

mosquitto_pub -h host -t beehive/data -m "{\"id\":123, \"temp_in\":5.42, \"temp_out\":6.42, \"humidity_in\":6.42, \"humidity_out\":6.42, \"bees_count\":2500, \"timestamp\":\"2018-01-19 03:14:07\"}" -p 1883 -u user -P password

