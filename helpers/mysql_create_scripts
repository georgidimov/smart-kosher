CREATE TABLE weights (id INT NOT NULL, weight DOUBLE NOT NULL, timestamp TIMESTAMP NOT NULL PRIMARY KEY) ;

CREATE TABLE battery_percentage (id INT NOT NULL, battery_percentage INT NOT NULL, timestamp TIMESTAMP NOT NULL PRIMARY KEY) ;

CREATE TABLE locations (id INT NOT NULL, longitude DOUBLE NOT NULL, latitude DOUBLE NOT NULL, timestamp TIMESTAMP NOT NULL PRIMARY KEY) ;

CREATE TABLE data (id INT NOT NULL, temp_in DOUBLE NOT NULL, temp_out DOUBLE NOT NULL, humidity_in DOUBLE NOT NULL, humidity_out DOUBLE NOT NULL, bees_count INT NOT NULL, timestamp TIMESTAMP NOT NULL PRIMARY KEY) ;
