CREATE TABLE weights (id INT NOT NULL PRIMARY KEY, weight DOUBLE NOT NULL) ;

CREATE TABLE locations (id INT NOT NULL PRIMARY KEY, longitude DOUBLE NOT NULL, latitude DOUBLE NOT NULL) ;

CREATE TABLE data (id INT NOT NULL PRIMARY KEY, temp_in DOUBLE NOT NULL, temp_out DOUBLE NOT NULL, humidity_in DOUBLE NOT NULL, humidity_out DOUBLE NOT NULL, bees_count INT NOT NULL, timestamp TIMESTAMP NOT NULL) ;
